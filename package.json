{
  "name": "merlins-config-manager",
  "version": "1.0.3",
  "description": "A simple configuration manager that uses the advantages of typescript to easily create templated config sections.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diesermerlin/merlins-config-manager.git"
  },
  "scripts": {
    "prepublish": "npm build",
    "build": "tsc && typedoc --out docs src",
    "check": "gts check",
    "clean": "gts clean",
