    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run check"
  },
  "keywords": [],
  "author": "Merlin Westphal",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^13.11.1",
    "typedoc": "^0.19.0",
    "typescript": "^3.8.3",
    "ts-node": "^9.0.0",
    "gts": "^2.0.2"
  },
  "bugs": {
    "url": "https://github.com/diesermerlin/merlins-config-manager/issues"
  },
